<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("partials/head"); %>
  </head>
  <body>
    <div class="container">
      <main>
        <div class="main-container">
          <div class="login-container">
            <h1>Log in</h1>
            <form class="login-form">
              <input id="email" class="input" name="email" type="email" placeholder="Your email" />
              <input
                id="password"
                class="input"
                name="password"
                type="password"
                placeholder="Your password"
              />
              <button type="submit">Log in</button>
            </form>
          </div>
        </div>
      </main>
    </div>
    <script src="/login.js"></script>
  </body>
</html>
